  </property>
  
  <property name="lt2">
  <column name="WMM_LT"/>
  </property>
  
  <property name="ft1">
  <column name="WBM_FT"/>
  </property>
  
  <property name="ft2">
  <column name="WMM_FT"/>
  </property>
  
 </class>
</hibernate-mapping>