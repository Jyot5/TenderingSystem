<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.GSubGradeVO" table="RT_GSubGrade_mst">
   <id name="csgid" type="long" column="CSG_ID" >
   <generator class="increment"/>
  </id>

  <property name="sf">
   <column name="F_Sampling" />
  </property>
  
  <property name="sc">
  <column name="C_Sampling"/>
  </property>
  
  <property name="ltf">
  <column name="F_LT"/>
  </property>
  
  <property name="ltc">
  <column name="C_LT"/>
  </property>
  
  <property name="ftf">
  <column name="F_FT"/>
  </property>
  
  <property name="ftc">
  <column name="C_FT"/>
  </property>
  
   <property name="inote">
  <column name="Instruction_Note"/>
  </property>
 
  <property name="remark">
  <column name="Remark"/>
  </property>

<many-to-one name="tvo" class="VO.TenderVO" column="tender_Id" cascade="delete"></many-to-one>
 
 </class>
</hibernate-mapping>